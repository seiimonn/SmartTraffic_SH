  <row>
    <panel>
      <input type="dropdown" token="tok_time_series" searchWhenChanged="true">
        <label>Data Series</label>
        <fieldForLabel>column</fieldForLabel>
        <fieldForValue>column</fieldForValue>
        <search>
          <query>index=hack simId = "$tok_simulation_1$"
| table *
| transpose
| table column
| search column IN ("last*","emissions","Noise","FuelConsumption","WaitingTime")</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
      </input>
      <input type="dropdown" searchWhenChanged="true">
        <label>Mode</label>
        <choice value="avg">Average</choice>
        <choice value="cum">Cumulative</choice>
        <change>
          <condition label="Average">
            <set token="showAvg">1</set>
            <unset token="showCum"></unset>
          </condition>
          <condition label="Cumulative">
            <set token="showCum">1</set>
            <unset token="showAvg"></unset>
          </condition>
        </change>
      </input>
      <input type="text" token="tok_time_span" searchWhenChanged="true">
        <label>Span</label>
        <default>1</default>
        <initialValue>1</initialValue>
      </input>
    </panel>
  </row>
  <row>
    <panel depends="$showAvg$">
      <title>Simulation Avg by all Edges</title>
      <chart>
        <search>
          <query>index=hack simId IN ("$tok_simulation_1$","$tok_simulation_2$")
| eval _time = now() - simSteps + simStep
| timechart span="$tok_time_span$" avg($tok_time_series$) AS $tok_time_series$ by simId
| eval simulation_1 = '$tok_simulation_1$',
       simulation_2 = '$tok_simulation_2$'
| where isnotnull(simulation_1) AND isnotnull(simulation_2)
| table _time simulation_1 simulation_2</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">1</option>
        <option name="charting.chart">line</option>
        <option name="charting.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
    <panel depends="$showCum$">
      <title>Simulation Cumulative by all Edges</title>
      <chart>
        <search>
          <query>index=hack simId IN ("$tok_simulation_1$","$tok_simulation_2$")
| eval _time = now() - simSteps + simStep
| timechart span="$tok_time_span$" avg($tok_time_series$) AS $tok_time_series$ by simId
| eval simulation_1 = '$tok_simulation_1$',
       simulation_2 = '$tok_simulation_2$'
| where isnotnull(simulation_1) AND isnotnull(simulation_2)
| table _time simulation_1 simulation_2
| streamstats sum(simulation_1) AS simulation_1 sum(simulation_2) AS simulation_2</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">1</option>
        <option name="charting.chart">line</option>
        <option name="charting.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <title>Edge Drilldown</title>
      <input type="dropdown" token="tok_time_func" searchWhenChanged="true">
        <label>Function</label>
        <choice value="avg">Average</choice>
        <choice value="cum">Cumulative</choice>
        <change>
          <condition label="Average">
            <set token="drilldownAvg">1</set>
            <unset token="drilldownCum"></unset>
          </condition>
          <condition label="Cumulative">
            <set token="drilldownCum">1</set>
            <unset token="drilldownAvg"></unset>
          </condition>
        </change>
      </input>
      <input type="dropdown" token="tok_time_edge" searchWhenChanged="true">
        <label>Edge</label>
        <choice value="*">All</choice>
        <default>*</default>
        <initialValue>*</initialValue>
        <fieldForLabel>edgeId</fieldForLabel>
        <fieldForValue>edgeId</fieldForValue>
        <search>
          <query>index=hack simId IN ("$tok_simulation_1$","$tok_simulation_2$")
| stats count by edgeId</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
      </input>
    </panel>
  </row>
  <row>
    <panel depends="$drilldownAvg$">
      <title>Simulation Avg by $tok_time_edge$</title>
      <chart>
        <search>
          <query>index=hack simId IN ("$tok_simulation_1$","$tok_simulation_2$") edgeId="$tok_time_edge$"
| eval _time = now() - simSteps + simStep
| timechart span="$tok_time_span$" avg($tok_time_series$) AS $tok_time_series$ by simId
| eval simulation_1 = '$tok_simulation_1$',
       simulation_2 = '$tok_simulation_2$'
| where isnotnull(simulation_1) AND isnotnull(simulation_2)
| table _time simulation_1 simulation_2</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="charting.chart">line</option>
        <option name="charting.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
    <panel depends="$drilldownCum$">
      <title>Simulation Cumulative by $tok_time_edge$</title>
      <chart>
        <search>
          <query>index=hack simId IN ("$tok_simulation_1$","$tok_simulation_2$") edgeId="$tok_time_edge$"
| eval _time = now() - simSteps + simStep
| timechart span="$tok_time_span$" avg($tok_time_series$) AS $tok_time_series$ by simId
| eval simulation_1 = '$tok_simulation_1$',
       simulation_2 = '$tok_simulation_2$'
| where isnotnull(simulation_1) AND isnotnull(simulation_2)
| table _time simulation_1 simulation_2</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="charting.chart">line</option>
        <option name="charting.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
  </row>
</form>